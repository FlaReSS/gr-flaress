<?xml version="1.0"?>
<block>
  <name>vector sink</name>
  <key>flaress_vector_sink_x</key>
  <category>[flaress]</category>
  <import>import flaress</import>
	<make>blocks.vector_sink_$(type.fcn)($vlen, $reserve_items)</make>
  <param>
		<name>Input Type</name>
		<key>type</key>
		<type>enum</type>
		<option>
			<name>Int64</name>
			<key>s64</key>
			<opt>fcn:int64</opt>
		</option>
		<option>
			<name>Double</name>
			<key>f64</key>
			<opt>fcn:double</opt>
		</option>
	</param>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<param>
		<name>Reserve memory for items</name>
		<key>reserve_items</key>
		<value>1024</value>
		<type>int</type>
		<hide>part</hide>
	</param>
	<check>$vlen &gt; 0</check>
	<sink>
		<name>in</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
	</sink>
</block>
