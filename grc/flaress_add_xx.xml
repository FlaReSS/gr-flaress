<?xml version="1.0"?>
<block>
  <name>Add</name>
  <key>flaress_add_double</key>
  <category>[flaress]</category>
  <import>import flaress</import>
  <make>flaress.add_$(type.fcn)($vlen)</make>
	<param>
		<name>IO Type</name>
		<key>type</key>
		<type>enum</type>
    <option>
      <name>Float64</name>
      <key>f64</key>
      <opt>fcn:double</opt>
    </option>
    <option>
      <name>Int64</name>
      <key>s64</key>
      <opt>fcn:int64</opt>
    </option>
	</param>
	<param>
		<name>Num Inputs</name>
		<key>num_inputs</key>
		<value>2</value>
		<type>int</type>
	</param>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<check>$num_inputs &gt; 1</check>
	<check>$vlen &gt; 0</check>
	<sink>
		<name>in</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
		<nports>$num_inputs</nports>
	</sink>
	<source>
		<name>out</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
	</source>
</block>
