<?xml version="1.0"?>
<block>
  <name>Integer Math</name>
  <key>flaress_integer_math_xx</key>
  <category>[flaress]</category>
  <import>import flaress</import>
  <make>flaress.integer_math_$(type.fcn)($vlen, $N_int)</make>
  <param>
		<name>Input Type</name>
		<key>type</key>
    <value>int</value>
		<type>enum</type>
		<option>
			<name>Integer</name>
			<key>int</key>
			<opt>fcn:cc</opt>
		</option>
		<option>
			<name>Integer64</name>
			<key>s64</key>
			<opt>fcn:ll</opt>
		</option>
	</param>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
  <param>
    <name>N of bits integer part</name>
    <key>N_int</key>
    <type>int</type>
  </param>

	<check>$vlen &gt; 0</check>

  <sink>
		<name>in</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
	</sink>
	
	<source>
		<name>in</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
	</source>
</block>
