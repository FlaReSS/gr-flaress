<?xml version="1.0"?>
<block>
  <name>Add Const</name>
  <key>flaress_add_const_int64</key>
  <category>[flaress]</category>
  <import>import flaress</import>
  <make>flaress.add_const_$(type.fcn)($const, $vlen)</make>
	<callback>set_k($const)</callback>
	<param>
		<name>IO Type</name>
		<key>type</key>
		<type>enum</type>
    <option>
      <name>Float64</name>
      <key>f64</key>
      <opt>fcn:double</opt>
    </option>
    <option>
      <name>Int64</name>
      <key>s64</key>
      <opt>fcn:int64</opt>
    </option>
	</param>
	<param>
		<name>Constant</name>
		<key>const</key>
		<value>0</value>
		<type>$type.const_type</type>
	</param>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<check>len($const) == $vlen</check>
	<check>$vlen &gt; 0</check>
	<sink>
		<name>in</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
	</sink>
	<source>
		<name>out</name>
		<type>$type</type>
		<vlen>$vlen</vlen>
	</source>
</block>
